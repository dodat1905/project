<% provide :title, t(".all_users") %>
<h1><%= t ".all_users" %></h1>

<%= paginate @users %>

<ul class="users">
  <h3><%= "Import User" %></h3>
  <%= form_tag imports_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "import", class: "btn btn-success" %>
  <% end %>
  <%= render @users %>
</ul>

<%= paginate @users %>
